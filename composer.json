{
    "name": "zahidcse/secretmanager",
    "description": "Aws Secret Manager",
    "keywords": ["laravel", "Aws Secret Manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "zahid hasan",
            "email": "zahiislink@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0||~6.0||~7.0||~8.0"
    },
    "autoload": {
        "psr-4": {
            "Zahidcse\\Secretmanager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zahidcse\\": "tests/"
        }
    },
    "suggest": {
        "zahidcse/Secretmanager": "Simple hello word package" 
    },
    "minimum-stability": "stable",  
    "extra": {
        "laravel": {
            "providers": [
                "Zahidcse\\Secretmanager\\SecretmanagerServiceProvider"
            ]
        }
    }
}
